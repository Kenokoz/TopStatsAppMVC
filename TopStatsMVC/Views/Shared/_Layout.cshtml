<body>
    <header class="header">
        <div class="header__inner">
            <div class="logo">
                <a class="header__link" asp-area="" asp-controller="GetStats" asp-action="Index">
                    <img class="logo__img" src="Images/logoCS.png" />Top<span class="stats__span">Stats</span>
                </a>
            </div>
            <div class="nav">
                <a id="stat" class="nav__link" asp-area="" asp-controller="GetStats" asp-action="Index">Get stats</a>
                <a id="players" class="nav__link" asp-area="" asp-controller="Players" asp-action="Index">Top players</a>
                <a id="info" class="nav__link" asp-area="" asp-controller="Information" asp-action="Index">Information</a>
            </div>
        </div>
    </header>

    @RenderBody()

    <div class="footer">
        <div class="container">
            <div class="footer__inner">
                <div class="logo">
                    <a class="header__link" href="#">
                        <img class="logo__img" src="Images/logoCS.png" />Top<span class="stats__span">Stats</span>
                    </a>
                </div>
                <div class="footer__share">
                    <div class="footer__title">Share</div>
                    <div class="social__footer">
                        <a class="social__item" href="#" target="_blank">
                            <svg class="social__icon">
                                <use xlink:href="#vk"></use>
                            </svg>
                        </a>
                        <a class="social__item" href="#" target="_blank">
                            <svg class="social__icon">
                                <use xlink:href="#facebook"></use>
                            </svg>
                        </a>
                        <a class="social__item" href="#" target="_blank">
                            <svg class="social__icon">
                                <use xlink:href="#twitter"></use>
                            </svg>
                        </a>
                        <a class="social__item" href="#" target="_blank">
                            <svg class="social__icon">
                                <use xlink:href="#linkedin"></use>
                            </svg>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="copyright">
        <div class="container">
            <div class="copyright__text">
                <div>Copyright © 2020 TOP<span class="stats__span">Stats</span> All Rights Reserved</div>
                <div>Made by Kenokoz</div>
            </div>
        </div>
    </div>

    <script src="~/js/modules/header.js"></script>
    @*<script src="~/js/modules/rows.js"></script>
